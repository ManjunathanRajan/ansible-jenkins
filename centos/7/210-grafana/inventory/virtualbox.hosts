[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[jenkins_controllers]
jenkins ansible_ssh_host=192.168.32.13 ansible_ssh_port=22 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/jenkins/virtualbox/private_key

[agents]
agent1 ansible_ssh_host=192.168.32.15 ansible_ssh_port=22 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/agent1/virtualbox/private_key

[grafanas]
grafana ansible_ssh_host=192.168.32.11 ansible_ssh_port=22 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/grafana/virtualbox/private_key